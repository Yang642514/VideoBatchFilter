{
  "columns": {
    "link": "vediolink",
    "title": "video_title",
    "duration": "duration",
    "resolution": "max_resolution",
    "resolution_ok": "resolution_ok",
    "violence_blood": "has_violence_blood",
    "smoking": "has_smoking",
    "overall_pass": "overall_pass",
    "notes": "notes",
    "error": "error_message"
  },
  "filters": {
    "min_resolution": [1920, 1080],
    "min_duration": 10,
    "max_duration": 3600,
    "min_size": 5,
    "max_size": 2000
  },
  "video_extensions": [".mp4", ".avi", ".mkv", ".mov", ".flv", ".wmv"],
  "detector": {
    "clip_model_id": "openai/clip-vit-base-patch32",
    "enabled_labels": ["blood", "gore", "violence", "smoking"],
    "clip_thresholds": {
      "blood": 0.28,
      "gore": 0.30,
      "violence": 0.30,
      "smoking": 0.32
    },
    "thumbnail_samples": 5
  },
  "downloader": {
    "use_download_sections": true,
    "section_strategy": "uniform",
    "max_concurrent_downloads": 3
  },
  "concurrency": {
    "max_info_workers": 10,
    "max_detect_workers": 3
  },
  "device": {
    "prefer": "cuda"
  },
  "auth": {
    "cookies_path": "cookies.txt"
  },
  "logging": {
    "level": "INFO",
    "file": "video_filter.log"
  },
  "processing": {
    "max_retries": 3,
    "retry_delay": 1.0,
    "timeout": 30,
    "batch_size": 100
  }
}